package model;

import java.util.ArrayList;
import java.util.List;

public class User {
	private String name;
	private String id;
	private String domian;
	private String email;
	private String role;
	private List<Observant> normal;
	private List<Observant> suspect;
	private List<Observant> daily;
	
	public User (){
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getDomian() {
		return domian;
	}

	public void setDomian(String domian) {
		this.domian = domian;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getRole() {
		return role;
	}

	public void setRole(String role) {
		this.role = role;
	}

	public List<Observant> getNormal() {
		return normal;
	}

	public void setNormal(List<Observant> normal) {
		this.normal = normal;
	}

	public List<Observant> getSuspect() {
		return suspect;
	}

	public void setSuspect(List<Observant> suspect) {
		this.suspect = suspect;
	}

	public List<Observant> getDaily() {
		return daily;
	}

	public void setDaily(List<Observant> daily) {
		this.daily = daily;
	}
	
	
}
