package model;

import java.util.ArrayList;
import java.util.List;

public class Device {
	private String id;
	
	public Device() {
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}
	
	public void putDevice( Device device ) {
		if( this.right == null ) {
			this.right = device;
		}else {
			Node next = this.right.right;
			while( next.getRight() != null ) {
				next = next.right;
			}
			next.setRight( device );
		}
	}
}
